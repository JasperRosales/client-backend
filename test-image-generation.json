{
  "generate_image": {
    "endpoint": "POST /api/image/generate",
    "request": {
      "prompt": "A cute cat sitting on a wall"
    }
  },
  "edit_image": {
    "endpoint": "POST /api/image/edit",
    "request": {
      "image": "data:image/png;base64,YOUR_BASE64_IMAGE",
      "prompt": "Add a small knitted wizard hat on the cat's head"
    }
  }
}

